ul
  each key, i in keys
    li
      svg(role="presentation" width="1.5em" height="0.75em")
        if type === "solid"
          rect(width="100%" height="100%" fill=colors && colors[i])
        if type === "line"
          line(x2="100%" y1="50%" y2="50%" stroke=colors && colors[i])
      
      span!= key
